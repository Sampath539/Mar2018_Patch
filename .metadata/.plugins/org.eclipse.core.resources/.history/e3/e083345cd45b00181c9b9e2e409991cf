package com.java;

public class Q2{

public static void main(String[] args) {

}
}

class A{
	
	public String doStuff(String a) {
		return a;
	}
}
class B extends A{
public String doStuff(String a) {
	return a.replace('a', 'e');
}
}
class C extends B{
	public String doStuff(String a) {
		return a.substring(2);
	}
}