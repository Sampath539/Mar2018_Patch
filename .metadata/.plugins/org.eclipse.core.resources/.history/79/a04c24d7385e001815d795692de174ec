package com.java;

public class Q52 extends Star implements Universe {

	public static void main(String[] args) {
		Q52 o1 = new Q52();
		Star o2 = o1;
		((Q52)o2).doStuff();
		((Star) o1).doStuff();
		((Universe)o1).doStuff();
	}

}

class Star{
	public void doStuff() {
		System.out.println("Twinkling star");
	}
}
interface Universe{
	public void doStuff();
}
class Sun extends Star implements Universe{
	
}