package com.java;

public class Q12 {

	public static void main(String[] args) {
		int a[][] = new int[1][3];
		
		//System.out.println(a.length);
		for(int i=0;i<a.length;i++) {
			System.out.println(a[i].toString());
			for(int j=0;j<a[i].length;j++) {
				//System.out.println(a[i].length);
				a[i][j] =10;
				//System.out.println("a["+i+"]["+j+"]");
			}
		}

	}

}
