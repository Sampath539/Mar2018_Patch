package com.java;

public class Q45 {

	public static void m1(int[] a) {
		for(int j=0;j<a.length;j++) {
			a[j]=a[j]+1;
		}
	}
	public static void main(String[] args) {
		// TODO Auto-generated method stub
		List<Integer> g=new ArrayList<>();
int[] a = {10,20,30};
m1(a);
for(int c:a) {
	System.out.println(c);
}
	}

}
